/**app.wxss**/
@import './miniprogram_npm/tdesign-miniprogram/common/style/theme/_index.wxss';
@import './miniprogram_npm/tdesign-miniprogram/common/style/theme/_dark.wxss';
@import "./unocss.wxss";

.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}

.title {
    margin-left: 8px;
    font-size: 18px;
    font-weight: 600;
}

.t-result__description {
    white-space: pre-wrap;
    word-break: break-all;
}

.t-message__text {
    white-space: nowrap;
}

Page {
    background-color: var(--td-bg-color-secondarycontainer);
    --td-navbar-bg-color: var(--td-bg-color-secondarycontainer);
    --td-input-align-items: start;
}

@media (prefers-color-scheme: dark)  {
    Page{
        background-color: var(--td-bg-color-page);
        --td-navbar-bg-color: var(--td-bg-color-page);
    }
}




